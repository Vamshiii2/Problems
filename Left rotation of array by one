// Left rotate the array by one
#include <iostream>
using namespace std;

int main() {

    int size;
    cout<<"Enter the size:";
    cin>>size;
    int arr[size];
    cout<<"Enter the array elements:";
    for(int i=0;i<size;i++)
    {
        cin>>arr[i];
    }
    int temp = arr[0];
    for(int i=0;i<size;i++)
    {
        arr[i] = arr[i+1];
        
    }
    arr[size-1] = temp;
   cout<<"The left of an array by one is:";
    for(int i=0;i<size;i++)
    {
        cout<<arr[i]<<" ";
    }
    

    return 0;
}
